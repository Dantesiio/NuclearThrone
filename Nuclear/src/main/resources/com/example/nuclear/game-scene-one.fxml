<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.image.ImageView?>

<AnchorPane fx:id="fondoNivel" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.nuclear.GameSceneOne"
            prefHeight="600.0" prefWidth="800.0">
    <children>
        <Canvas fx:id="canvas" height="1080.0" width="1920.0"
                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <ImageView fx:id="fondoIMGVIEW" fitHeight="1080.0" fitWidth="1920.0"
                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    </children>
</AnchorPane>
